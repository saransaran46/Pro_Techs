import java.util.Scanner;
public class Main
{
	public static void main(String[] args)
	{
		Scanner sc = new Scanner(System.in);
		String str = sc.nextLine();
		char ch[]=str.toCharArray();
		int count=0;
		for(int i=0;i<str.length();i++)
		{
		    if(ch[i]>=48 && ch[i]<=57)
		    {
		        count++;
		    }
		}
		if(count == str.length())
		{
		    System.out.print("digis yes");
		}
	    else{
	        System.out.print("no");
	    }
	}
}
